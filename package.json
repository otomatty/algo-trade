{
  "name": "algo-trade",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:tauri": "tauri dev",
    "build": "tsc && vite build",
    "build:tauri": "tauri build",
    "preview": "vite preview",
    "tauri": "tauri",
    "test": "bun test --dom",
    "test:all": "bun test --dom && bun run test:python",
    "test:ts": "bun test --dom",
    "test:ts:watch": "bun test --watch --dom",
    "test:ts:coverage": "bun test --coverage --dom",
    "test:python": "cd src-python && python3 -m pytest",
    "test:python:unit": "cd src-python && python3 -m pytest -m unit",
    "test:python:integration": "cd src-python && python3 -m pytest -m integration",
    "test:python:cov": "cd src-python && python3 -m pytest --cov=. --cov-report=html",
    "test:python:watch": "cd src-python && python3 -m pytest --watch",
    "test:python:verbose": "cd src-python && python3 -m pytest -v",
    "test:python:csv": "cd src-python && python3 -m pytest tests/unit/test_csv_importer.py",
    "test:python:api": "cd src-python && python3 -m pytest tests/unit/test_api_clients.py",
    "test:python:collector": "cd src-python && python3 -m pytest tests/unit/test_data_collector.py",
    "test:python:db": "cd src-python && python3 -m pytest tests/unit/test_database_operations.py tests/unit/test_database_schema.py",
    "test:python:flow": "cd src-python && python3 -m pytest tests/integration/test_data_collection_flow.py"
  },
  "dependencies": {
    "@mantine/carousel": "^8.3.6",

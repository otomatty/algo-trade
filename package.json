    "@testing-library/user-event": "^14.5.0",
    "@types/jsdom": "^27.0.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "bun-types": "^1.3.1",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
